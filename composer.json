{
    "name": "walissonaguirra/ci-less",
    "description": "CodeIgniter4 Less",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "codeigniter4/framework": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Config\\": "app/Config"
        },
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
